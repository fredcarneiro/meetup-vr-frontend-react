  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fredcarneiro/meetup-vr-frontend-react/issues"
  },
  "homepage": "https://github.com/fredcarneiro/meetup-vr-frontend-react#readme",
  "dependencies": {
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  }
